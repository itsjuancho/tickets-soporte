Options +FollowSymLinks
RewriteEngine on
RewriteRule ^ticket/([0-9]+)$ viewTicket.php?ticket_id=$1
RewriteRule ^validar_respuesta$ replyAsAdmin.php
RewriteRule ^cerrar_ticket$ cerrar.php
RewriteRule ^abrir_ticket$ abrir.php
RewriteRule ^editar_ticket$ editar.php
RewriteRule ^atender_ticket atender.php